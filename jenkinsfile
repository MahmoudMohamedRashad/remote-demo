node{
    git branch: "main", url: ""
    stage("build"){
        try{
            sh'echo"your build stage sucess!"'
        } 
        catch(Exception error){
            sh'echo"build stage faild"'
        }
    stage("testing"){
        if(env.BRANCH_NAME == "main"){
            sh'echo"hello from main! your test sucess"'
        }
        else{
            sh'echo"branch error"'
        }
    }
    }
 }
